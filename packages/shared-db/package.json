{
  "name": "@curatedotfun/shared-db",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rspack && tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .ts",
    "db:generate": "pnpm exec drizzle-kit generate",
    "db:migrate": "pnpm exec drizzle-kit migrate",
    "db:studio": "pnpm exec drizzle-kit studio",
    "seed:dev": "bun ./scripts/seed-dev.ts",
    "seed:remote": "bun ./scripts/seed-remote.ts",
    "overwrite-vars:up": "bun run ./scripts/overwrite-vars.ts up",
    "overwrite-vars:down": "bun run ./scripts/overwrite-vars.ts down"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "postgres": "^3.4.7",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@rspack/cli": "1.2.8",
    "@rspack/core": "1.2.8",
    "@types/async-retry": "^1.4.9",
    "@types/bun": "latest",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "pg": "^8.15.6",
    "pg-connection-string": "^2.9.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.43.1",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "package.json"
  ]
}
