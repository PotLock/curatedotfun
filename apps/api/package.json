    "@types/mustache": "^4.2.6",
    "axios": "^1.6.8",
    "concurrently": "^9.1.2",
    "drizzle-seed": "^0.3.1",
    "nock": "^13.5.4",
    "typescript": "^5.3.3",
    "wait-on": "^8.0.2",
    "zod": "^3.25.62"
  },
  "dependencies": {
    "@crosspost/sdk": "^0.3.0",
    "@crosspost/types": "^0.3.0",
    "@crosspost/scheduler-sdk": "^0.1.1",
    "@curatedotfun/shared-db": "workspace:*",
